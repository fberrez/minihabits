{
    "$schema": "https://json.schemastore.org/swcrc",
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "tsx": false,
        "decorators": true,
        "dynamicImport": true
      },
      "target": "es2022",
      "loose": false,
      "externalHelpers": false,
      "keepClassNames": true,
      "preserveAllComments": false,
      "experimental": {
        "plugins": [
          [
            "swc-plugin-pre-paths",
            {
              "baseUrl": ".",
              "paths": {
                "src/*": ["./src/*"]
              }
            }
          ]
        ]
      }
    },
    "module": {
      "type": "commonjs",
      "strict": false,
      "strictMode": true,
      "lazy": false,
      "noInterop": false
    },
    "minify": false,
    "sourceMaps": true
  }