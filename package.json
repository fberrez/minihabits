{
  "name": "minihabits",
  "version": "0.0.1",
  "description": "MiniHabits Monorepo - Backend and Frontend",
  "author": "",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "start": "turbo start",
    "lint": "turbo lint",
    "test": "turbo test",
    "type-check": "turbo type-check",
    "clean": "turbo clean",
    "dev:backend": "pnpm dev --filter=minihabits-backend",
    "dev:frontend": "pnpm dev --filter=minihabits-frontend",
    "start:backend": "pnpm start --filter=minihabits-backend",
    "start:frontend": "pnpm start --filter=minihabits-frontend",
    "build:backend": "pnpm build --filter=minihabits-backend",
    "build:frontend": "pnpm build --filter=minihabits-frontend",
    "generate-api:frontend": "pnpm generate-api --filter=minihabits-frontend"
  },
  "devDependencies": {
    "turbo": "^2.5.5",
    "typescript": "^5.1.3",
    "webpack-cli": "^6.0.1"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.13.1"
}
